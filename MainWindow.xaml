<Window x:Class="_123456ModernFamily.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_123456ModernFamily"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <TextBox x:Name="txtEpisodeName" />
        <Button x:Name="btnFullOutput" Click="btnFullOutput_Click">Full Output</Button>
        <Button x:Name="btnGetSeason" Click="btnGetSeason_Click">Get Season and number</Button>
        <TextBlock x:Name="txtOutput" TextWrapping="Wrap" />
    </StackPanel>
</Window>
